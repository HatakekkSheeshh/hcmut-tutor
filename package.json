{
  "name": "tutor-support-system",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "api": "tsx server.ts",
    "dev:api": "tsx watch server.ts",
    "ws": "tsx ws-server/index.ts",
    "dev:ws": "tsx watch ws-server/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:api": "tsx scripts/testAPIs.ts",
    "test:management": "tsx scripts/testManagementAPIs.ts",
    "test:management:clean": "npm run seed && npm run test:management",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "seed": "tsx lib/seed.ts",
    "seed:clean": "node -e \"require('fs').rmSync('data', {recursive: true, force: true})\" && npm run seed",
    "seed:forum": "tsx scripts/seed-forum.ts",
    "validate": "tsx scripts/validate.ts",
    "stats": "tsx -e \"import('./lib/dataValidator').then(m => m.generateDataStats())\"",
    "migrate:mongodb": "tsx scripts/migrate-to-mongodb.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google/generative-ai": "^0.24.1",
    "@gsap/react": "^2.1.2",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-charts": "^8.14.0",
    "@mui/x-date-pickers": "^8.14.1",
    "@mui/x-date-pickers-pro": "^8.14.1",
    "@tanstack/react-query": "^5.90.9",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "emoji-picker-react": "^4.15.0",
    "express": "^5.1.0",
    "gsap": "^3.13.0",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.294.0",
    "mongodb": "^7.0.0",
    "nanoid": "^5.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.2.3",
    "react-router-dom": "^6.20.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "yaml": "^2.8.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/ui": "^4.0.9",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.20.6",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^4.0.9"
  }
}
